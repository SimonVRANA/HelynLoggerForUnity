      "b": 0.0156862754,
      "a": 1.0
    },
    "ErrorColor": {
      "r": 1.0,
      "g": 0.0,
      "b": 0.0,
      "a": 1.0
    },
    "AssertColor": {
      "r": 1.0,
      "g": 0.0,
      "b": 0.0,
      "a": 1.0
    },
    "ExceptionColor": {
      "r": 1.0,
      "g": 0.0,
      "b": 1.0,
      "a": 1.0
    },
    "NoneColor": {
      "r": 1.0,
      "g": 1.0,
      "b": 1.0,
      "a": 1.0
    }
  },
  "EnableFileLogging": false,
  "LogFilePath": "logs/log.txt",

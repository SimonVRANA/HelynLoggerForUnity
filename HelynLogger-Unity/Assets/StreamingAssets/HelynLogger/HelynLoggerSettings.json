      "a": 1.0
    },
    "AssertColor": {
      "r": 1.0,
      "g": 0.0,
      "b": 0.0,
      "a": 1.0
    },
    "ExceptionColor": {
      "r": 1.0,
      "g": 0.0,
      "b": 1.0,

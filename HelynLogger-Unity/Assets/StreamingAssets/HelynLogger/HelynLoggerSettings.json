    "Format": "[{timestamp:yyyy-MM-dd HH:mm:ss.fff}] {level} {category}: {message}",
    "EnableColorLogLevel": false,
    "LogColor": {
      "r": 0.0,
      "g": 0.0,
      "b": 0.0,
      "a": 1.0
    },
    "WarningColor": {
